import java.util.*;

class Test{
      public static void main(String[] args) {
            int i1=1;
            for(int i =1; i<=5; i++){
                  if(i==i1){
                        System.out.println("  1 2 3 4 5");
                  }
                  System.out.print(i+" ");

                  for(int j= 1; j<=5; j++){
                        System.out.print(j+" ");

                        
                  }
                  System.out.println(" ");
            }
      }
}

