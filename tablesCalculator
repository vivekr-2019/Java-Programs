import java.util.*;

class Test{
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        System.out.print("Which Table? ");
        int i =sc.nextInt();
        int j =1;

        for(j=1;j<=20;j++){
            System.out.println(i+" * "+j+" = "+i*j);
        }

    }
}
