import java.util.*;

class Test{
      public static void main(String[] args) {
            //int count=0;
            for(int i =1; i<=5; i++){
                  
                  for (int j=1; j<=5;j++){
                        if(j+i>5){
                              System.out.print("* ");
                        }
                        else{
                              System.out.print("  ");
                        }
                  }
                  for(int k=2;k<=i;k++){
                        System.err.print("* ");
                        }

                  System.out.println("");
            }

            for(int l =2; l<=5; l++){
                  
                  for (int m=1; m<=5;m++){
                        if(m>=l){
                              System.out.print("* ");
                        }
                        else{
                              System.out.print("  ");
                        }
                  }
                  for(int n=2;n<=5-1+l;n++){
                        System.out.print("* ");
                        }

                  System.out.println("");
            }


      }
}




